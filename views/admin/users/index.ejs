<%- include('../../partials/header') -%>
<%- include('../../partials/navbar') -%>

<div class="container">
    <hr>
    <h2>Usuários</h2>
    <br><a class="btn btn-primary" href="/admin/users/create">Criar novo usuário</a>
    <hr>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Email</th>
            </tr>
        </thead>

        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <th><%= user.id %></th>
                    <th><%= user.email %></th>
                </tr>
            <% }) %>  
        </tbody>
    </table>
</div>



<%- include('../../partials/footer') -%>